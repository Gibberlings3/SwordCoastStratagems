  OR(2)
    ActionListEmpty()
    Specifics(Myself,SPIRIT)
  !StateCheck(LastSeenBy(Myself),STATE_REALLY_DEAD)
  Allegiance(LastSeenBy(Myself),EVILCUTOFF)
  !InParty(LastSeenBy(Myself))
THEN
  RESPONSE #100
    AttackOneRound(LastSeenBy(Myself))
END

