DEFINE_ACTION_FUNCTION deployment BEGIN

     LAF ssl_to_bcs STR_VAR script=dw#shdlg location=~genai/ssl~ END

     LAF edit_creature STR_VAR creature=~amaran islsir izefia kahrk neira nexlit nimbul ragefa
                                        takiyah tarnes telano teven benjy coksmth droth
                                        ogrmba osmadi slythe zordra~
                               tv=1
                               editstring=~swap_script=>"%tutu_var%initdlg=>dw#shdlg"~
     END

     LAF edit_creature STR_VAR creature=~%tutu_scripto%grema02 %tutu_scripto%grema03 %tutu_scripto%grema_d~
                               editstring=~swap_script=>"%tutu_var%initdlg=>dw#shdlg"~
     END

    MAKE_PATCH
       tamoko=>dw#tamok
       marek=>dw#marek
       silke=>dw#silke
       tranzi=>dw#tranz
       mulahe=>dw#mudep
    END
    ACTION_PHP_EACH patch_data AS creature=>script BEGIN
       LAF ssl_to_bcs STR_VAR inline=yes script END
       LAF edit_creature STR_VAR creature tv=1 editstring= ~strip_script=>%script% insert_script_high=>%script%~ END
    END


